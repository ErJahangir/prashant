{
  "name": "sakeenah",
  "description": "Modern Hindu wedding invitation platform with multi-tenant architecture",
  "version": "2.1.0",
  "author": "mrofisr",
  "license": "Apache-2.0",
  "keywords": [
    "wedding",
    "invitation",
    "hindu",
    "react",
    "vite",
    "hono",
    "postgresql",
    "multi-tenant"
  ],
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:client": "vite",
    "build": "vite build",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "preview": "vite preview",
    "generate-links": "bun generate-links-example.js",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:unit": "vitest run",
    "migrate:pg-to-firestore": "node scripts/migrate-postgres-to-firestore.js",
    "seed:firestore": "node scripts/seed-firestore.js",
    "test:e2e": "vitest run e2e/**/*.e2e.spec.js",
    "test:coverage": "vitest run --coverage",
    "prepare": "husky"
  },
  "dependencies": {
    "@tanstack/react-query": "^5.90.20",
    "caniuse-lite": "^1.0.30001757",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "dotenv": "^16.4.5",
    "firebase": "^10.14.1",
    "framer-motion": "^11.15.0",
    "lucide-react": "^0.469.0",
    "react": "^18.3.1",
    "react-confetti": "^6.1.0",
    "react-dom": "^18.3.1",
    "react-helmet-async": "^2.0.5",
    "react-router-dom": "^7.1.1",
    "tailwind-merge": "^2.6.0",
    "tailwindcss-animate": "^1.0.7",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20251128.0",
    "@eslint/js": "^9.17.0",
    "@types/react": "^18.3.18",
    "@types/react-dom": "^18.3.5",
    "@vitejs/plugin-react": "^4.3.4",
    "@vitest/coverage-v8": "^2.1.0",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.17.0",
    "eslint-plugin-react": "^7.37.2",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.16",
    "firebase-admin": "^11.11.0",
    "globals": "^15.14.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "postcss": "^8.4.49",
    "prettier": "^3.4.2",
    "tailwindcss": "^3.4.17",
    "terser": "^5.46.0",
    "vite": "^6.0.5",
    "vitest": "^2.1.0"
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,css}": [
      "prettier --write"
    ]
  }
}
